{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.development next dev",
    "dev:staging": "env-cmd -f .env.staging next dev",
    "dev:production": "env-cmd -f .env.production next dev",
    "build": "prisma generate && next build",
    "build:development": "env-cmd -f .env.development npm run build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "build:production": "env-cmd -f .env.production npm run build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test:db": "env-cmd -f .env.development node scripts/test-db.js",
    "verify:dev": "env-cmd -f .env.development node scripts/verify-env.js",
    "verify:staging": "env-cmd -f .env.staging node scripts/verify-env.js",
    "verify:prod": "env-cmd -f .env.production node scripts/verify-env.js",
    "db:seed": "node prisma/seed.mjs",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.971.0",
    "@aws-sdk/client-ses": "^3.971.0",
    "@aws-sdk/s3-request-presigner": "^3.971.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/dompurify": "^3.0.5",
    "@types/js-cookie": "^3.0.6",
    "@types/sanitize-html": "^2.16.0",
    "@types/validator": "^13.15.10",
    "bcryptjs": "^3.0.3",
    "dompurify": "^3.3.1",
    "ioredis": "^5.9.2",
    "isomorphic-dompurify": "^2.35.0",
    "jose": "^6.1.3",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.3",
    "next": "16.0.10",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.71.1",
    "react-hot-toast": "^2.6.0",
    "sanitize-html": "^2.17.0",
    "swr": "^2.3.8",
    "validator": "^13.15.26"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/nextjs-vite": "^10.1.11",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "25.0.3",
    "@types/react-dom": "^19",
    "@vitest/browser-playwright": "^4.0.17",
    "@vitest/coverage-v8": "^4.0.17",
    "dotenv": "^17.2.3",
    "env-cmd": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "^10.1.11",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "playwright": "^1.57.0",
    "storybook": "^10.1.11",
    "tailwindcss": "^4",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
