{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.development next dev",
    "dev:staging": "env-cmd -f .env.staging next dev",
    "dev:production": "env-cmd -f .env.production next dev",
    "build": "prisma generate && next build",
    "build:development": "env-cmd -f .env.development npm run build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "build:production": "env-cmd -f .env.production npm run build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test:db": "env-cmd -f .env.development node scripts/test-db.js",
    "verify:dev": "env-cmd -f .env.development node scripts/verify-env.js",
    "verify:staging": "env-cmd -f .env.staging node scripts/verify-env.js",
    "verify:prod": "env-cmd -f .env.production node scripts/verify-env.js"
  },
  "dependencies": {
    "@prisma/client": "^7.2.0",
    "next": "16.0.10",
    "prisma": "^7.2.0",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "25.0.3",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "env-cmd": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "tailwindcss": "^4"
  }
}
